{
  "expo": {
    "splash": {
      "image": "./assets/images/logo@3x.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "name": "AIMicromind",
    "slug": "AIMicromind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo@3x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anaseltanany.AImicromind"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon/splash-icon.png",

          "resizeMode": "center",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
