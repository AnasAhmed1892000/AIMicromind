{
  "expo": {
    "splash": {
      "image": "./assets/images/logo@3x.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "name": "AI MicroMind",
    "slug": "AIMicroMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo@3x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aimicromind.AIMicromind"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#000000"
      },
      "package": "com.aimicromind.AIMicromind"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo@3x.png",
          "resizeMode": "center",
          "backgroundColor": "#000000"
        }
      ],
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
